{
  "name": "easy-etn-wallet",
  "version": "1.0.0-beta",
  "description": "Unoffical Electroneum Desktop Wallet",
  "homepage": "https://github.com/travis1234/easy-etn-wallet.git",
  "author": {
    "name": "Travis Winsor",
    "email": "travis_winsor@hotmail.com"
  },
  "keywords": [
    "angular",
    "angular 5",
    "electron",
    "typescript",
    "sass"
  ],
  "main": "main.js",
  "private": true,
  "scripts": {
    "postinstall": "npx electron-builder install-app-deps",
    "ng": "ng",
    "start": "node hooks/environments/set_profile.js && npm-run-all -p ng:serve electron:serve",
    "build": "node hooks/environments/set_profile.js && ng build && npm run electron:tsc",
    "build:prod": "node hooks/environments/set_profile.js && ng build --prod && npm run electron:tsc",
    "ng:serve": "ng serve --host localhost --port 4200 --proxy-config proxy.conf.js",
    "electron:tsc": "tsc main.ts",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && npx electron-builder build --linux",
    "electron:windows": "npm run build:prod && npx electron-builder build --windows",
    "electron:mac": "npm run build:prod && npx electron-builder build --mac",
    "test": "karma start ./karma.conf.js",
    "pree2e:build": "webdriver-manager update --gecko false && npm run ng:serve",
    "pree2e:protractor": "wait-on http-get://localhost:4200/ && protractor ./protractor.conf.js",
    "e2e": "npm-run-all -p pree2e:build pree2e:protractor"
  },
  "dependencies": {
    "@angular/forms": "^5.2.9",
    "@ng-bootstrap/ng-bootstrap": "^1.1.0",
    "@ng-select/ng-select": "^0.30.1",
    "@ngx-formly/bootstrap": "^2.0.0-rc.51",
    "@ngx-formly/core": "^2.0.0-rc.51",
    "angular-datatables": "^5.0.0",
    "angularx-qrcode": "^1.1.0",
    "datatables.net": "^1.10.16",
    "datatables.net-dt": "^1.10.16",
    "electron-settings": "^3.1.4",
    "electron-titlebar": "0.0.3",
    "jquery": "^3.3.1",
    "ngx-clipboard": "^10.0.0",
    "ngx-warehouse": "^1.0.2",
    "rx-monero-wallet": "^1.0.15"
  },
  "devDependencies": {
    "@angular/animations": "^5.2.9",
    "@angular/cli": "1.7.3",
    "@angular/common": "5.2.9",
    "@angular/compiler": "5.2.9",
    "@angular/compiler-cli": "5.2.9",
    "@angular/core": "5.2.9",
    "@angular/forms": "5.2.9",
    "@angular/http": "5.2.9",
    "@angular/language-service": "5.2.9",
    "@angular/platform-browser": "5.2.9",
    "@angular/platform-browser-dynamic": "5.2.9",
    "@angular/router": "5.2.9",
    "@ngx-translate/core": "9.1.1",
    "@ngx-translate/http-loader": "2.0.1",
    "@types/core-js": "0.9.36",
    "@types/datatables.net": "^1.10.9",
    "@types/jasmine": "2.8.6",
    "@types/jasminewd2": "2.0.3",
    "@types/jquery": "^3.3.1",
    "@types/node": "7.0.7",
    "angular2-moment": "1.8.0",
    "bootstrap": "4.0.0",
    "codelyzer": "4.2.1",
    "copyfiles": "2.0.0",
    "core-js": "2.5.3",
    "cross-env": "5.1.4",
    "dotenv": "5.0.1",
    "electron": "1.8.4",
    "electron-builder": "20.5.1",
    "electron-reload": "1.2.2",
    "enhanced-resolve": "4.0.0",
    "fs-extra": "5.0.0",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "jquery": "3.3.1",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "1.4.2",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "1.0.0",
    "npm-run-all": "4.1.2",
    "npx": "10.0.1",
    "popper.js": "1.14.1",
    "protractor": "5.3.0",
    "replace": "0.3.0",
    "rxjs": "5.5.7",
    "ts-node": "5.0.1",
    "tslint": "5.9.1",
    "typescript": "2.6.2",
    "wait-on": "^2.1.0",
    "webdriver-manager": "12.0.6",
    "zone.js": "0.8.20"
  }
}
